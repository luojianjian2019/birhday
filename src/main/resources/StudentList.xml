<workbook>
  <worksheet idx="0">
    <section startRow="0" endRow="1"/>
    <loop startRow="2" endRow="4" items="students" var="student" varType="birthday.greetings.Student">
      <section startRow="2" endRow="4">
        <mapping row="2" col="1">product.stuNo</mapping>
        <mapping row="2" col="2">product.name</mapping>
        <mapping row="2" col="3">product.sex</mapping>
      </section>
      <loopbreakcondition>
        <rowcheck offset="0">
          <cellcheck offset="0" />
        </rowcheck>
      </loopbreakcondition>
    </loop>
  </worksheet>
</workbook>

